    "favicons-webpack-plugin": "0.0.6",
    "file-loader": "^0.9.0",
    "gh-pages": "^0.11.0",
    "handlebars": "^4.0.5",
    "handlebars-loader": "^1.3.0",
    "html-webpack-plugin": "^2.22.0",
    "node-sass": "^3.8.0",
    "rimraf": "^2.5.4",
    "sass-loader": "^3.2.3",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
